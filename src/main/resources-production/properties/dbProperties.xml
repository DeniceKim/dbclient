<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>DB 커넥션 연결정보 PRODUCT </comment>
	<!-- driver 정보  -->
	<entry key="dbClient.database.driver">org.sqlite.JDBC</entry>
		<!-- 접속정보 : 배포시에 삭제되지 않도록 배포 경로 밖에다가 설정 바랍니다. -->
	<entry key="dbClient.database.url">jdbc:sqlite:/data/dbclient/dbClient.db</entry>

	<!-- driver 정보  -->
	<entry key="dbClientLog.database.driver">org.sqlite.JDBC</entry>
	<!-- 접속정보 : 경로를 수정하세요.-->
	<entry key="dbClientLog.database.url">jdbc:sqlite:/data/dbclient/dbClientLog.db</entry>

</properties>